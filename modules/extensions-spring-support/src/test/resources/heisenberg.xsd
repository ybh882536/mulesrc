                    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/extension/heisenberg" type="org.mule.module.extensions.HealthStatusEnumType" use="required" name="finalHealth"></xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="org.mule.module.extensions.Door">

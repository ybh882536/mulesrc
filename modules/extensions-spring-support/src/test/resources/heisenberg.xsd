                    <xs:annotation>
                        <xs:documentation>Default configuration</xs:documentation>
                    </xs:annotation>
                    <xs:sequence>
                        <xs:element minOccurs="0" maxOccurs="1" name="enemies">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element minOccurs="0" maxOccurs="unbounded" name="enemy">
                                        <xs:complexType>
                                            <xs:simpleContent>
                                                <xs:extension base="xs:string">
                                                    <xs:attribute type="mule:expression" use="required" name="value"></xs:attribute>
                                                </xs:extension>
                                            </xs:simpleContent>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element minOccurs="0" maxOccurs="1" name="ricin-packs">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element minOccurs="0" maxOccurs="unbounded" name="ricin-pack">
                                        <xs:complexType>
                                            <xs:complexContent>
                                                <xs:extension xmlns="http://www.mulesoft.org/schema/mule/extension/heisenberg" base="org.mule.module.extensions.Ricin"></xs:extension>
                                            </xs:complexContent>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element minOccurs="0" maxOccurs="1" name="next-door">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension xmlns="http://www.mulesoft.org/schema/mule/extension/heisenberg" base="org.mule.module.extensions.Door"></xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute type="xs:string" use="required" name="name"></xs:attribute>
                    <xs:attribute type="mule:expression" use="optional" name="myName"></xs:attribute>
                    <xs:attribute type="mule:expression" use="optional" name="age"></xs:attribute>
                    <xs:attribute type="mule:expression" use="optional" name="enemies"></xs:attribute>
                    <xs:attribute type="mule:expression" use="required" name="money"></xs:attribute>
                    <xs:attribute type="mule:expression" use="required" name="cancer"></xs:attribute>
                    <xs:attribute type="mule:expression" use="required" name="dateOfBirth"></xs:attribute>
                    <xs:attribute type="mule:expression" use="required" name="dateOfDeath"></xs:attribute>
                    <xs:attribute type="mule:expression" use="optional" name="recipe"></xs:attribute>
                    <xs:attribute type="mule:expression" use="optional" name="ricinPacks"></xs:attribute>
                    <xs:attribute type="mule:expression" use="optional" name="nextDoor"></xs:attribute>
                    <xs:attribute type="mule:expression" use="optional" name="candidateDoors"></xs:attribute>
